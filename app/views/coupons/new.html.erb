<h1>Coupon Form</h1>

<form action="<%= coupons_path %>" method="POST">
    <label>Coupon Code: </label><br>
    <input type="text" id="coupon_code" name="coupon[coupon_code]">
<br>
    <label>Store Name: </label><br>
    <input type="text" id="store" name="coupon[store]">
<br><br>
     <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="submit" value="Submit Coupon">
</form>
