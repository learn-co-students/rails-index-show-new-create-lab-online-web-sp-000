will render form that uses form_tag method <br>

<h3>Coupon Form</h3>
<%= form_tag post_create_path do %>
    <label>Coupon Code: </label>
    <%= text_field_tag :'coupon[coupon_code]' %><br>

    <label>Store: </label>
    <%= text_field_tag :'coupon[store]' %><br>

    <%= submit_tag "Submit Coupon" %>
<% end %>